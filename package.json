{
  "name": "pocketmanager-node",
  "version": "0.0.1",
  "description": "A Mithril.js / node.js project",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "debug": "set DEBUG=PocketManager-Node:* & node --inspect ./bin/www",
    "run-webpack": "webpack -d --watch",
    "watch-sass": "sass --watch source/scss/style.scss:public/assets/style.css",
    "build": "webpack -p",
    "build-css": "sass source/scss/style.scss:public/assets/style.css && postcss --use autoprefixer -o public/assets/style.prefix.css public/assets/style.css && postcss --use cssnano -o public/assets/style.min.css public/assets/style.prefix.css --no-map",
    "LiveReloadJS": "livereload public"
  },
  "browserslist": ["last 4 version"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/pro-light-svg-icons": "^5.2.0",
    "@fortawesome/pro-regular-svg-icons": "^5.2.0",
    "@fortawesome/pro-solid-svg-icons": "^5.2.0",
    "animejs": "^2.2.0",
    "bootstrap": "^4.1.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-http2-workaround": "^1.1.3",
    "express-sanitizer": "^1.0.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "leaflet": "^1.3.3",
    "mithril": "^1.1.6",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "mysql": "^2.16.0",
    "node-sass-middleware": "0.11.0",
    "perfect-scrollbar": "^1.4.0",
    "popper.js": "^1.14.4",
    "tooltip.js": "^1.2.0",
    "uid-generator": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "eslint": "^5.4.0",
    "html-webpack-plugin": "^3.2.0",
    "livereload": "^0.7.0",
    "node-sass": "^4.9.3",
    "ospec": "^3.0.1",
    "sass": "^1.13.0",
    "sass-loader": "^7.0.3",
    "postcss-cli": "^6.0.0",
    "postcss": "^7.0.2",
    "autoprefixer": "^9.1.5",
    "cssnano": "^4.1.0",
    "string-replace-loader": "^2.1.1",
    "uglify-js": "^3.4.5",
    "uglifycss": "0.0.29",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
}
